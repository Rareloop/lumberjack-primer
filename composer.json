    "require": {
        "rareloop/primer-core": "dev-dev@dev",
        "rareloop/primer-frontend": "dev-master@dev",
        "rareloop/lumberjack-core": "^4.0.0",
        "gajus/dindent": "^2.0",
        "composer/composer": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "satooshi/php-coveralls": "^1.0",
        "mockery/mockery": "^1.0.0",
        "brain/monkey": "^2.0.2",
